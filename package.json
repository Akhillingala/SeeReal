{
  "name": "seereal-extension",
  "version": "0.1.0",
  "description": "Real-time 3D transparency metrics for news articles",
  "private": true,
  "scripts": {
    "postinstall": "node scripts/generate-icons.js",
    "test:gemini": "node scripts/test-gemini.js",
    "build": "node scripts/generate-icons.js && webpack --mode production",
    "dev": "node scripts/generate-icons.js && DOTENV_CONFIG_PATH=.env.local node -r dotenv/config node_modules/.bin/webpack --mode development --watch",
    "package": "npm run build && node scripts/package.js"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@react-three/drei": "^9.96.0",
    "@react-three/fiber": "^8.15.0",
    "framer-motion": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "three": "^0.160.0",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.268",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/three": "^0.160.0",
    "autoprefixer": "^10.4.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.0",
    "dotenv": "^16.3.0",
    "html-webpack-plugin": "^5.5.0",
    "postcss-loader": "^7.3.0",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.4.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
  }
}
